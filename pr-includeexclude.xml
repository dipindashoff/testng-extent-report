<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="TestNGPractice">
	<test name="Include-Exclude">
		<classes>
			<class name="testng.practice.IncludeAndExclude">
				<methods>
					<include name="include"></include>
					<exclude name="exclude"></exclude>
				</methods>
			</class>
		</classes>
	</test>
</suite>